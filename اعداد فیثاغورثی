#include <iostream>
using namespace std;
int main()
{
  long long int my_a=0,ashkan=0;
  long long int a,b,c;
  cin>>a>>b>>c;
  if(a>b && a>c)
  {
      if(a*a==(b*b)+(c*c))
      {
          cout<<"YES";
          ashkan++;
      }
      else
      {
          cout<<"NO";
          ashkan++;
      }
  }
  if(b>a && b>c)
  {
     if(b*b==(a*a)+(c*c))
      {
          cout<<"YES";
          ashkan++;
      }
      else
      {
          cout<<"NO";
          ashkan++;
      }
  }
  if(c>a && c>b)
  {
    if(c*c==(a*a)+(b*b))
      {
          cout<<"YES";
          ashkan++;
      }
      else
      {
          cout<<"NO";
          ashkan++;
      }
  }
  if(ashkan==0)
  {
      cout<<"NO";
  }
}
