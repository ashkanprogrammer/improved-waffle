#include <iostream>
using namespace std;
int main()
{
   int n;
   int a=1, b=1;
   cin>>n;
   while(a<=n)
   {
       cout<<"*";
       a++;
   }
   a=1;
   cout<<"\n";
   for(int i=2;i<=n-1;i++)
   {
       cout<<"*";
       while(b<=n-2)
       {
         cout<<" ";
         b+=1;
       }
       b=1;
       cout<<"*";
       cout<<"\n";
   }
   while(a<=n)
   {
       cout<<"*";
       a++;
   }
}
